{
  "name": "eslint-config-ybiquitous",
  "version": "1.6.0",
  "description": "ybiquitous's ESLint configuration",
  "main": "index.js",
  "author": "ybiquitous <ybiquitous@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "base.js",
    "index.js",
    "postinstall.js"
  ],
  "repository": "ybiquitous/eslint-config-ybiquitous",
  "bugs": "https://github.com/ybiquitous/eslint-config-ybiquitous/issues",
  "homepage": "https://github.com/ybiquitous/eslint-config-ybiquitous#readme",
  "dependencies": {
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "3",
    "husky": "*",
    "lint-staged": "*",
    "standard-version": "*",
    "validate-commit-msg": "*"
  },
  "peerDependencies": {
    "eslint": "3"
  },
  "scripts": {
    "postinstall": "node ./postinstall.js",
    "lint": "eslint --max-warnings=0 --ignore-path=.gitignore .",
    "pretest": "npm run lint",
    "test": "./test.sh",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0",
      "git add"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  }
}
