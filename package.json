{
  "name": "eslint-config-ybiquitous",
  "version": "6.2.0",
  "description": "Personal ESLint configuration",
  "author": "Masafumi Koba <ybiquitous@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "main": "index.js",
  "files": [
    "node.js",
    "browser.js",
    "react.js",
    "rules"
  ],
  "repository": "ybiquitous/eslint-config-ybiquitous",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-eslint-comments": "^3.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-restricted-globals": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "5.6.1",
    "eslint-find-rules": "3.3.1",
    "execa": "1.0.0",
    "prettier": "1.14.3",
    "tape": "4.9.1",
    "ybiq": "5.0.0"
  },
  "peerDependencies": {
    "eslint": ">=5.3.0 <6",
    "prettier": ">=1.14.0 <2"
  },
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx,.mjs .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:md": "markdownlint --ignore node_modules --ignore CHANGELOG.md \"**/*.md\"",
    "lint:md:fix": "prettier --write \"**/*.md\"",
    "lint": "npm-run-all --print-label --parallel lint:*",
    "pretest": "eslint --print-config .eslintrc* | eslint-config-prettier-check",
    "test": "tape \"test/**/*.test.js\"",
    "test:debug": "DEBUG=true npm test",
    "release": "standard-version --sign",
    "release:dry-run": "npm run release -- --dry-run",
    "find-new-rules": "eslint-find-rules --unused",
    "find-deprecated-rules": "eslint-find-rules --deprecated"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,mjs}": [
        "eslint --fix --no-ignore",
        "git add"
      ],
      "*.md": [
        "prettier --write",
        "markdownlint",
        "git add"
      ]
    },
    "ignore": [
      "CHANGELOG.md"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
