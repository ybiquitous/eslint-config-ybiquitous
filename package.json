{
  "name": "eslint-config-ybiquitous",
  "version": "4.2.0",
  "description": "ybiquitous's ESLint configuration",
  "main": "index.js",
  "author": "ybiquitous <ybiquitous@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "base.js",
    "index.js",
    "node.js"
  ],
  "repository": "ybiquitous/eslint-config-ybiquitous",
  "bugs": "https://github.com/ybiquitous/eslint-config-ybiquitous/issues",
  "homepage": "https://github.com/ybiquitous/eslint-config-ybiquitous#readme",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-eslint-comments": "^2.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "prettier": "^1.9.2",
    "ybiq": "*"
  },
  "peerDependencies": {
    "eslint": "4",
    "prettier": "1"
  },
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx,.mjs .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:md": "markdownlint *.md",
    "lint": "npm-run-all --print-name --print-label --parallel lint:*",
    "pretest": "eslint --print-config .eslintrc* | eslint-config-prettier-check",
    "test": "./test.sh",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run"
  },
  "lint-staged": {
    "*.{js,jsx,mjs}": [
      "eslint --fix --no-ignore",
      "git add"
    ],
    "*.md": "markdownlint"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prepend CHANGELOG.md \"<!-- markdownlint-disable -->\n\""
    },
    "message": "chore(release): new version %s"
  }
}
