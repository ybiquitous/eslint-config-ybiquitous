{
  "name": "eslint-config-ybiquitous",
  "version": "2.1.0",
  "description": "ybiquitous's ESLint configuration",
  "main": "index.js",
  "author": "ybiquitous <ybiquitous@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "files": [
    "base.js",
    "index.js"
  ],
  "repository": "ybiquitous/eslint-config-ybiquitous",
  "bugs": "https://github.com/ybiquitous/eslint-config-ybiquitous/issues",
  "homepage": "https://github.com/ybiquitous/eslint-config-ybiquitous#readme",
  "dependencies": {
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "*",
    "@commitlint/config-angular": "*",
    "eslint": "4",
    "husky": "*",
    "lint-staged": "*",
    "standard-version": "*"
  },
  "peerDependencies": {
    "eslint": "4"
  },
  "scripts": {
    "lint": "eslint --max-warnings=0 --ignore-path=.gitignore .",
    "test": "./test.sh",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0",
      "git add"
    ]
  }
